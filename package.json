{
  "private": true,
  "name": "cookas",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "/cookas",
  "scripts": {
    "start": "yarn run dev",
    "build": "preact build --no-prerender --template ./src/template.html",
    "predeploy": "yarn build",
    "deploy": "ghpages",
    "ghpages": "gh-pages -d ./build",
    "serve": "serve ./build",
    "dev": "preact watch --template ./src/template.html",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "gh-pages": "^2.0.1",
    "preact-cli": "@next",
    "preact-svg-loader": "^0.2.1"
  },
  "dependencies": {
    "cxs": "^6.2.0",
    "preact": "@next",
    "storeon": "@next"
  }
}
